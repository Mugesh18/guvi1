import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int b=1;
		int a;
		for(int i=1;i<=n;i++) {
			a=b*i;
			b=a;
			
			if(b==n) {
				System.out.print("It is in a factorial "+i);
				break;
			}
			else if(b>n){
				System.out.print("It is not in a factorial");
				break;
		}
		}
	}

}
